<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/doctor_dashboard_layout"
    tools:context=".ui.doctor.ui.main.DoctorDashboardFragment">

    <Button
        android:id="@+id/doctor_visit_list_button"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:text="Visit List"
        app:layout_constraintBottom_toTopOf="@+id/doctor_patient_list_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/doctor_patient_list_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Patient List"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doctor_visit_list_button" />
</androidx.constraintlayout.widget.ConstraintLayout>