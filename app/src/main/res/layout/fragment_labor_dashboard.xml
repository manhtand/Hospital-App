<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.service.labor.ui.main.LaborDashboardFragment">

    <Button
        android:id="@+id/labor_blood_test_button"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:text="Blood Test"
        app:layout_constraintBottom_toTopOf="@+id/labor_mri_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/labor_mri_button"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:text="MRI"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/labor_blood_test_button" />

</androidx.constraintlayout.widget.ConstraintLayout>