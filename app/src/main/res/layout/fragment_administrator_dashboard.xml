<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/administrator_dashboard_layout"
    tools:context=".ui.administrator.ui.main.AdminDashboardFragment">

    <Button
        android:id="@+id/administrator_bed_list_button"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:text="Bed List"
        app:layout_constraintBottom_toTopOf="@+id/administrator_patient_list_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/administrator_patient_list_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Patient List"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/administrator_bed_list_button" />
</androidx.constraintlayout.widget.ConstraintLayout>